<!doctype html>
<html lang="en">
	<%- include('header') -%>
	<body>
		<section class="prompt">
			<h3>todos</h3>
			<h1>Sign up</h1>
			<form action="/signup" method="post">
				<section>
					<label for="username">Username</label>
					<input id="username" name="lalala" type="text" autocomplete="username" required autofocus>
				</section>
				<section>
					<label for="current-password">Password</label>
					<input id="current-password" name="lololo" type="password" autocomplete="current-password" required>
				</section>
				<button type="submit">Sign in</button>
			</form>
			<hr>
			<p class="help">Already have an account? <a href="/login">Sign in</a></p>
		</section>
		<%- include('footer') -%>
	</body>
</html>
